{
  "name": "github_blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "bunx --bun vite --force",
    "preview": "bunx --bun vite preview",
    "sync": "./scripts/sync.sh",
    "build-only": "bunx --bun vite build",
    "build": "bun run sync && bun run build-only",
    "build-only:packs": "./scripts/build_packs.sh",
    "build:packs": "bun run sync && bun run build-only:packs",
    "build:all": "bun run sync && bun run build-only && bun run build-only:packs"
  },
  "dependencies": {
    "github-markdown-css": "^5.5.1",
    "highlight.js": "^11.9.0",
    "marked": "^12.0.1",
    "marked-admonition-extension": "^0.0.4",
    "marked-alert": "^2.0.1",
    "marked-code-format": "^1.1.4",
    "marked-emoji": "^1.4.0",
    "marked-extended-tables": "^1.0.8",
    "marked-footnote": "^1.2.2",
    "marked-gfm-heading-id": "^3.1.3",
    "marked-highlight": "^2.1.1",
    "unocss": "^0.58.5",
    "vue": "^3.3.2",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^6.0.1",
    "@types/bun": "^1.0.7",
    "@types/fs-extra": "^11.0.4",
    "@unocss/transformer-directives": "^0.58.6",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "fs-extra": "^11.2.0",
    "less": "^4.1.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.72.0",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
}
